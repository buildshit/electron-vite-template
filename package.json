{
  "name": "electron-vite-template",
  "version": "1.1.0",
  "description": "Reusable Electron + Vite starter template",
  "main": "scripts/create.cjs",
  "bin": {
    "electron-vite-template": "./scripts/create.cjs",
    "create-electron-vite-template": "./scripts/create.cjs"
  },
  "files": [
    "scripts/create.cjs",
    "electron",
    "src",
    "index.html",
    "tsconfig.json",
    "vite.config.ts",
    "template.gitignore",
    "bun.lock"
  ],
  "packageManager": "bun@1.3.6",
  "scripts": {
    "dev": "bun run build:electron && concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "bun run build:electron && vite build",
    "build:electron": "tsc -p electron",
    "preview": "vite preview",
    "electron": "electron .",
    "dist": "bun run build && electron-builder",
    "create": "node ./scripts/create.cjs"
  },
  "keywords": ["electron", "vite", "template"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.2.3",
    "concurrently": "^9.2.1",
    "electron": "40.4.1",
    "electron-builder": "^26.7.0",
    "typescript": "^5.9.3",
    "vite": "^7",
    "wait-on": "^9.0.4"
  },
  "dependencies": {}
}