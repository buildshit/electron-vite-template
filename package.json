{
  "name": "electron-vite-template",
  "version": "1.0.0",
  "description": "Electron app with Vite bundler",
  "main": "dist-electron/main.js",
  "packageManager": "bun@1.3.6",
  "scripts": {
    "dev": "bun run build:electron && concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "bun run build:electron && vite build",
    "build:electron": "tsc -p electron",
    "preview": "vite preview",
    "electron": "electron .",
    "dist": "bun run build && electron-builder"
  },
  "keywords": ["electron", "vite", "template"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.2.3",
    "concurrently": "^9.2.1",
    "electron": "^40.4.1",
    "electron-builder": "^26.7.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "wait-on": "^9.0.4"
  },
  "dependencies": {}
}